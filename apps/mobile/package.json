    "@types/react": "~18.2.14",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.1.3"
  },
  "scripts": {
    "start": "expo start --clear",
    "android": "expo start --android --clear",
    "ios": "expo start --ios --clear",
    "dev": "expo start --clear",
    "dev:android": "expo start --android --clear",
    "dev:ios": "expo start --ios --clear",
    "build": "tsc --noEmit",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "publish": "expo publish",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf node_modules && npm install",
    "reset": "expo start --clear --reset-cache",
    "deploy:staging": "echo 'Deploying to staging...'",
    "deploy:production": "echo 'Deploying to production...'",
    "health:check": "echo 'Claro Mobile App - Expo Development'"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
